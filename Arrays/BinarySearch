//binary search on sorted array
#include <iostream>
using namespace std;
int main(){
	int size;
	cout<<"Enter size: "<<endl;
	int Arr[size];
	cout<<"Enter ele in sortion";
	for(int i=0;i<size;i++){
		cin>>Arr[i];
		
	}
	int no;
	cout<<"Enter no:";
	cin>>no;
	int beg=0;
	int end=size-1;
	int mid;
	while(beg<=end){
	  mid=(beg+end)-1;
	
	if(Arr[mid]==no){
		cout<<(mid+1);
	}
	else if(no>Arr[mid]){
		beg=mid+1;
	}
	else if(no<Arr[mid]){
		end=mid-1;
		
	}

}
 cout<<"no is not there";
 return 0;
}
